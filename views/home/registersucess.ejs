</head>
<body>
    <h1>Registro Exitoso!</h1>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <% if(typeof alert != 'undefined') { %>
        <script>
            Swal.fire({
                    title:'<%= alertTitle %>',
                    text:'<%= alertMessage %>',
                    icon:'<%= alertIcon %>',
                    showConfirmaButton: '<%= showConfirmButton %>',
                    timer: '<%= timer %>',
                    }).then(()=>{
                window.location='/<%= ruta %>'
            })
            
        </script>
        <%}%>
</body>
</html>